            <limiterrormsg>AD Critical kerberos[88] responses approaching unacceptable level</limiterrormsg>
            <limitwarningmsg>AD Critical kerberos[88] responses at unacceptable level</limitwarningmsg>
          </attribute>
          <attribute>
            <object>53</object>
            <displayvalue>AD Critical dns[53]</displayvalue>
            <description>AD Critical dns[53]</description>
            <shortdescription/>
            <comment>type=java.lang.long</comment>
